<script>
function ValidateAlpha(evt)
{
    var keyCode = (evt.which) ? evt.which : evt.keyCode
    if ((keyCode < 65 || keyCode > 90) && (keyCode < 97 || keyCode > 123) && keyCode != 32)
     
    return false;
        return true;
}

 $("#txtContactNoLandingPage").keypress(function (e) {     
        var charCode = (e.which) ? e.which : event.keyCode 
        if (String.fromCharCode(charCode).match(/[^0-9]/g))
                return false; 
});

$("#btnSubmitLandingPage").click(function(){
    var error = 0;
    var errorArr = [];
    
    $(".requiredLandingPageField").each(function(){
        if($.trim($(this).val())==''||$.trim($(this).val())==null){
            $(this).css('border-color','#ff0000');
            error = 1;
            errorArr.push(error);
        }else{
            $(this).css('border-color','');
            error = 0;
            errorArr.push(error);
        }
    });
    
    var txtContactNoLandingPage = $("#txtContactNoLandingPage").val();
    if(txtContactNoLandingPage.length !== 10){
        $("#txtContactNoLandingPage").css('border-color','#ff0000');
        error = 1;
        errorArr.push(error);
    }else{
        $("#txtContactNoLandingPage").css('border-color','');
        error = 0;
        errorArr.push(error);
    }
    
    var email = $('#txtEmailLandingPage').val();
    var batposition = email.indexOf("@");  
    var bdotposition = email.lastIndexOf(".");  
    if(batposition<1 || bdotposition<batposition+2 || bdotposition+2>=email.length){  
        $('#txtEmailLandingPage').css('border-color','#ff0000');
        error = 1;
        errorArr.push(error);
    }else{
        $('#txtEmailLandingPage').css('border-color','');
        error = 0;
        errorArr.push(error);
    }
    
    
    if(errorArr.indexOf(1) !== -1){ 
      return false;
    }else{
        if (grecaptcha.getResponse()) {
              $("#LandingPageSpinner").css('display','inline-block');
              var form=$("#LandingPageForm");
              $.ajax({
                type:"POST",
                url:'LandingPageSubmit', 
                data:form.serialize(), 
                success:function(msg){
                    if(msg=='success'){
                        $("#LandingPageSpinner").css('display','none');
                        // $(".LandingPageSuccessMessage").css('display','block');
                        window.location.href = "https://asesasoft.com/thank-you";
                        grecaptcha.reset();
                        setTimeout(function() {
                            $(".LandingPageSuccessMessage").css('display','none');
                            $("#txtFirstNameLandingPage").val('');
                            $("#txtLastNameLandingPage").val('');
                            $("#txtContactNoLandingPage").val('');
                            $("#txtEmailLandingPage").val('');
                            $("#txtMessageLandingPage").val('');
                        }, 4000);
                    }else{
                        alert('Something went wrong.');
                    }
                }
              });
     	}else{
     	alert('Please confirm captcha to proceed');
     	}
    }
});    
</script>    
</body>
</html>
